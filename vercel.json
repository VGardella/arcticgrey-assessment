{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "framework": "remix",
    "outputDirectory": "dist",
    "routes": [
      { "handle": "dist/client/assets" },
      { "src": "/(.*)", "dest": "dist/server/index.js" }
    ]
  }